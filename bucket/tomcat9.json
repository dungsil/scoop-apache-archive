{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.108",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.108",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.108/bin/apache-tomcat-9.0.108-windows-x64.zip",
            "hash": "sha512:5e93f57a55b5c93491afb6f21a1ef24bccedc022fa0a3b43e76da454b69566d08807d453d4492657e9817b25d54e3c1a11920fef1ea745c99f81075a1047ba4b"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.108/bin/apache-tomcat-9.0.108-windows-x86.zip",
            "hash": "sha512:d36471aa1d0a4f1d915acb30c7f25135bdea32292e759d0c790b6145f5f2a6127df1467bd769ee1dab3f5fbaa90c21802c837008b504561aab6a0d64ec39d80f"
        }
    }
}
